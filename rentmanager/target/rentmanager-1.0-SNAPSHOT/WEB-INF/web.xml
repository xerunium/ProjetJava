<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.epf.rentmanager.servlet.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VehivleListeServlet</servlet-name>
		<servlet-class>com.epf.rentmanager.servlet.VehicleListeServletServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VehicleListeServlet</servlet-name>
		<url-pattern>/vehiclelist</url-pattern>
	</servlet-mapping>
</web-app>